<parameters>
  <parameter name="ConnectionString"
         description="ConnectionString"
         defaultValue="someValue">
    <parameterEntry kind="XmlFile" scope="Web.config"
                    match="/configuration/sblognetSettings/@connectionString" />
  </parameter>
  <parameter name="ApplicationInsightsInstrumentationKey AppSetting"
           description="ApplicationInsightsInstrumentationKey AppSetting"
           defaultValue="someValue">
    <parameterEntry kind="XmlFile" scope="Web.config"
                    match="/configuration/appSettings/add[@key='ApplicationInsightsInstrumentationKey']/@value" />
  </parameter>
</parameters>